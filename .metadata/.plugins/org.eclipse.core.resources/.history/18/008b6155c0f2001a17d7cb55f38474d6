package Exercícios_Lacos_Condicionais;

import java.util.Scanner;

public class LacosCondicionais4 {

	public static void main(String[] args) {
		/*
		 * 4-	Faça um programa em que permita a entrada de um número qualquer e 
		 * exiba se este número é par ou ímpar. Se for par 
		 * exiba também a raiz quadrada do mesmo; se for ímpar exiba 
		 * o número elevado ao quadrado.
		 */
		
		int numero;
		Scanner kb = new Scanner (System.in);
		
		System.out.println("Insura um número inteiro qualquer: ");
		numero = kb.nextInt(); 
		
		
		kb.close();
		
		

	}

}
